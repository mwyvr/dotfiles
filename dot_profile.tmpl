export EDITOR=nvim

# add path(s)
if [ "$(id -u)" -ne 0 ]; then
	mkdir -p $HOME/go/bin 
	PATH="$PATH:$HOME/go/bin"
	PATH="$PATH:$HOME/.local/bin"
    export PATH
