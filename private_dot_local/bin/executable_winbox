#!/bin/bash
FP="$HOME/.local/bin/winbox64.exe"
if [ ! -f "$FP" ]; then
	wget https://mt.lv/winbox64 -O $FP
fi
# wine may be exported/running from a distrobox
if [ -x ~/.local/bin/wine64 ]; then
	wine64 $FP
	distrobox-stop wine
else
	echo "Wine not found. Create a distrobox with wine and export."
fi
